Algoritmo Menu
	
	Escribir "Ingrese el numero de articulos "
	Leer nart
	Dimension articulos[nart]
	
	Para i<-1 Hasta nart Con Paso 1 Hacer
		Escribir "El nombre del articulo # ",i,""
		Leer articulos[i]
		
	Fin Para
	Para i<-1 Hasta nart Con Paso 1 Hacer
		Escribir "articulo ", articulos[i]
	Fin Para
	
FinAlgoritmo
